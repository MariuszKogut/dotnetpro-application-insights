{
  "name": "customer-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@microsoft/applicationinsights-web": "^2.4.4",
    "bootstrap": "^4.4.1",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "react-use": "^13.24.0",
    "typescript": "3.7.5"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/jest": "25.1.2",
    "@types/node": "13.7.1",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "concurrently": "^5.1.0",
    "nswag": "^13.2.3",
    "prettier": "^1.19.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:customer-backend": "cd ../backend/HS.CustomerApp.CustomerHost && dotnet run",
    "start:id-backend": "cd ../backend/HS.CustomerApp.IdHost && dotnet run",
    "start:all": "concurrently \"npm run start\" \"npm run start:customer-backend\" \"npm run start:id-backend\"",
    "build": "react-scripts build",
    "build:strict": "cross-env ci=true react-scripts build",
    "generate:client": "nswag run ../backend/openapi.nswag /runtime:NetCore30",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
